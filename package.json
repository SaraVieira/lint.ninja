{
  "name": "lint.ninja",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "eslint src",
    "test:size": "bundlesize",
    "start": "npm run -s serve",
    "build": "preact build --template src/index.html -p",
    "serve": "preact serve",
    "deploy": "npm run build && s3_website push ",
    "dev": "preact watch --template src/index.html"
  },
  "alias": "wuththelint",
  "keywords": [],
  "author": "Sara Vieira",
  "license": "MIT",
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "bundlesize": [
    {
      "path": "./build/bundle.*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./build/route-*.js",
      "maxSize": "35 kB"
    },
    {
      "path": "./build/sw.js",
      "maxSize": "3 kB"
    }
  ],
  "devDependencies": {
    "bundlesize": "^0.16.0",
    "eslint": "^4.3.0",
    "eslint-config-synacor": "^2.0.0",
    "if-env": "^1.0.0",
    "jest": "^22.0.6",
    "jest-image-snapshot": "^1.0.0",
    "less": "^3.0.0",
    "less-loader": "^4.0.5",
    "navalia": "^1.2.0",
    "npm-run-all": "^4.0.2",
    "offline-plugin": "^5.0.4",
    "preact-cli": "^2.0.0",
    "preact-cli-lodash": "^1.1.0"
  },
  "dependencies": {
    "animate-css-styled-components": "0.0.20",
    "axios": "^0.17.0",
    "preact": "^8.2.1",
    "preact-compat": "^3.16.0",
    "preact-render-to-string": "^3.7.0",
    "preact-router": "^2.5.5",
    "styled-components": "^3.1.3"
  }
}
